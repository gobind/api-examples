    	<conf name="build" description="build examples"/>
    </configurations>

    <dependencies defaultconfmapping="*->default">
		<!-- jersey dependencies -->
        <dependency org="javax.ws.rs" name="jsr311-api" rev="1.1" conf="*"/>
        <dependency org="com.sun.jersey" name="jersey-json" rev="1.1.2-ea" conf="*"/>
        <dependency org="com.sun.jersey" name="jersey-client" rev="1.1.2-ea" conf="*"/>
        <dependency org="com.sun.jersey" name="jersey-core" rev="1.1.2-ea" conf="*"/>
		
    </dependencies>
</ivy-module>
